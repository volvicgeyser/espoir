#include"stdafx.h"
#include"GameMain.h"
#include"debug.h"
namespace espoir{

	
GameMain::GameMain(SPDXInfo info){
	this->dinfo_ = info;
}

//ゲームの描画
void GameMain::Render(){
	Rect rect;
	rect.h = 100;
	rect.w = 100;
	rect.x = 50;
	rect.y = 50;
	
	if(this->dinfo_->g != NULL)
		this->dinfo_->g->DrawCircle(rect);
	else{
		DOut dout;
		dout << "graphicがNULLです " << __FILE__ << std::endl;
	}
}

//ゲームのアップデート処理
void GameMain::Update(){
	
}


}